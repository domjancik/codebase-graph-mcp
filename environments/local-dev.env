# Local Development Environment Configuration
# Full access for development and debugging

# No filtering - allow all commands
MCP_FILTER_MODE=none

# Enable debugging to see what commands are being executed
MCP_DEBUG_FILTERING=false

# Warn about unknown fields but don't crash
MCP_WARN_UNKNOWN_FIELDS=true
MCP_STRICT_MODE=false

# Validate configuration to catch issues early
MCP_VALIDATE_CONFIG=true
